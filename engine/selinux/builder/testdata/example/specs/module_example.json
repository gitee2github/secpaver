{
    "applicationList":[
        {
            "application":{
                "path":"$(BIN)/example",
                "user":"root",
                "launchers":[
                    "/bin/bash"
                ],
                "isPermissive":false
            },
            "permissionList":[
                {
                    "type":"filesystem",
                    "resources":[
                        "$(RESOURCE)"
                    ],
                    "actions":[
                        "create",
                        "read",
                        "write"
                    ]
                },
                {
                    "type":"network",
                    "resources":[
                        "domain:unix,type:stream,protocol:"
                    ],
                    "actions":[
                        "accept",
                        "bind",
                        "connect",
                        "listen",
                        "receive",
                        "send",
                        "listen"
                    ]
                },
                {
                    "type":"capability",
                    "resources":[

                    ],
                    "actions":[
                        "chown",
                        "dac_override",
                        "dac_read_search"
                    ]
                },
                {
                    "type":"rlimit",
                    "resources":[
                        "rlimit_stack"
                    ],
                    "actions":[
                        "10M"
                    ]
                }
            ]
        }
    ]
}
